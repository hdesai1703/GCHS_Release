    <formulas>
        <name>frm_HasPatientImpactValues</name>
        <dataType>Boolean</dataType>
        <expression>OR(
   ISPICKVAL( {!$Record.OLY_Patient_Impacted__c}, &apos;Yes&apos;),
   ISPICKVAL( {!$Record.OLY_Death_Injury_or_Infection_Likely__c}, &apos;Yes&apos;),
   ISPICKVAL( {!$Record.OLY_Death_Injury_or_Infection_Occurred__c}, &apos;Yes&apos;),
   FIND( &apos;[&apos; &amp; TEXT({!$Record.OLY_Patient_Outcome__c}) &amp; &apos;]&apos;, {!Get_No_Impact_Outcomes.OLY_Value__c})=0
)</expression>
    </formulas>
    <formulas>
        <name>frm_MeetsClosureRequirements</name>
        <dataType>Boolean</dataType>
        <expression>OR (
AND(
     NOT( {!frm_UnmetCustomerLetterRequirements} ),
     NOT( {!$Record.OLY_Has_Pending_Action__c} ) 
),
     {!AUTOCLOSURE_ACTIVE_FLAG}=False
)</expression>
    </formulas>
    <formulas>
        <name>frm_OpenOrInProgress</name>
        <dataType>Boolean</dataType>
        <expression>OR(
    {!$Record.CMPL123CME__CMPL123_WF_Status__c} = &apos;Open&apos;,
    {!$Record.CMPL123CME__CMPL123_WF_Status__c} = &apos;Complaint in Progress&apos;
)</expression>
    </formulas>
    <formulas>
        <name>frm_OpenOrSubmitting</name>
        <dataType>Boolean</dataType>
        <expression>OR(
    {!$Record.CMPL123CME__CMPL123_WF_Status__c} = &apos;Open&apos;,
    AND(
        ISCHANGED( {!$Record.CMPL123CME__CMPL123_WF_Action__c} ),
        {!$Record.CMPL123CME__CMPL123_WF_Action__c} = &apos;Submit&apos;
    )
)</expression>
    </formulas>
    <formulas>
        <name>frm_ProductReturnExpected</name>
        <dataType>Boolean</dataType>
        <expression>OR(
    ISPICKVAL({!$Record.OLY_Product_Return_Expected__c}, &apos;Yes&apos; ),
    ISPICKVAL({!$Record.OLY_Product_Return_Expected__c}, &apos;Other&apos; )
)</expression>
    </formulas>
    <formulas>
        <name>frm_UnmetCustomerLetterRequirements</name>
        <dataType>Boolean</dataType>
        <expression>AND(
    ISBLANK({!$Record.OLY_Customer_Letter_Sent_Date__c}),
   OR(ISPICKVAL({!$Record.OLY_Customer_Letter_Required__c},&apos;Yes&apos;),
         {!$Record.OLY_Customer_Letter_Required_Regulation__c}=TRUE)
 )</expression>
    </formulas>
